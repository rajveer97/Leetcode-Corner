class Solution {
public:
    vector<vector<int>> findDifference(vector<int>& nums1, vector<int>& nums2) {
        unordered_set<int> set1(nums1.begin(), nums1.end());
        unordered_set<int> set2(nums2.begin(), nums2.end());

        vector<int> dist_element1, dist_element2;
        for(auto x : set1) {
            if(set2.count(x) == 0) dist_element1.push_back(x);
        }

        for(auto x : set2) {
            if(set1.count(x) == 0) dist_element2.push_back(x);
        }

        return {dist_element1, dist_element2};
    }
};
